{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "dockerfile": "./Dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "/usr/sbin/nginx -g 'daemon off;'",
    "healthchecks": {
      "readiness": {
        "path": "/",
        "interval": 10
      }
    }
  }
}
